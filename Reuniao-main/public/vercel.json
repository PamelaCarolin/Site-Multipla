{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/feliz1", "dest": "/feliz1/feliz1.html" },
    { "src": "/papelaria", "dest": "/papelaria/pagina1.html" },
    { "src": "/reuniao", "dest": "Reuniao-main/public/reuniao.html" },
    { "src": "/api/agendar", "dest": "Reuniao-main/api/agendar.js" },
    { "src": "/api/cancelar", "dest": "Reuniao-main/api/cancelar.js" },
    { "src": "/api/consultar", "dest": "Reuniao-main/api/consultar.js" },
    { "src": "/api/products", "dest": "Reuniao-main/api/products.js" }
  ]
}
